{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "server/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/allTodos",
      "dest": "/server/src/index.ts"
    },
    {
      "src": "/createTodo",
      "dest": "/server/src/index.ts"
    },
    {
      "src": "/editTodo/:id",
      "dest": "/server/src/index.ts"
    },
    {
      "src": "/deleteTodo/:id",
      "dest": "/server/src/index.ts"
    }
  ]
}
